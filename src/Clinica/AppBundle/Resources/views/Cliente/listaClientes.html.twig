{# empty Twig template #}
{% extends 'base.html.twig' %}
{% block body %}
    <div class="large-6 columns">
        <table>
            <tr>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>Telefono</th>    
            </tr>
            {% for cliente in clientes %}
                <tr>
                    <td>{{cliente.nombre}}</td>
                    <td>{{cliente.apellidos}}</td>
                    <td>{{cliente.telefono}}</td>
                </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}


